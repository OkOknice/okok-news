.tabList-wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1px solid #f5f5f5;
}
.tabList-wrapper .tab-scroll-view {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  overflow: hidden;
}
.tabList-wrapper .tab-scroll-view .tab-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  height: 45px;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.tabList-wrapper .tab-scroll-view .tab-box .tab-item {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  padding: 0 10px;
  font-size: 14px;
  color: #333;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.tabList-wrapper .tab-scroll-view .tab-box .tab-item.active {
  color: #F07373;
}
.tabList-wrapper .tab-icons {
  position: relative;
  width: 45px;
  height: 45px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tabList-wrapper .tab-icons::after {
  content: ' ';
  position: absolute;
  left: 0;
  top: 12px;
  bottom: 12px;
  width: 1px;
  background-color: #ddd;
}

